{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "outDir": "bin",
    "rootDir": "src",
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["src/enforcepm.ts"]
}
